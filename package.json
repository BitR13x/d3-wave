{
  "name": "d3-wave",
  "version": "1.0.2",
  "description": "D3.js and ELK based schematic visualizer",
  "main": "index.js",
  "scripts": {
    "init": "eslint --init",
    "start": "babel src -D -d dist --watch",
    "build": "babel src -D -d dist",
    "compile": "npm run build",
    "clean": "rm -rf dist/ node_modules/",
    "prepare": "npm run build",
    "pretest": "rm -rf dist && mkdir dist && rollup -c --banner \"$(preamble)\"",
    "test": "jest",
    "coverage": "jest --coverage",
    "coveralls": "jest --coverage && cat ./tests/coverage/lcov.info | coveralls",
    "lint": "eslint src test --ignore-pattern tests/coverage*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nic30/d3-wave.git"
  },
  "keywords": [
    "D3.js",
    "hardware",
    "wave",
    "signal",
    "RTL",
    "viewer",
    "GTKWave"
  ],
  "author": "Michal Orsak",
  "contributors": ["Nic30"],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/Nic30/d3-wave/issues"
  },
  "homepage": "https://github.com/Nic30/d3-wave#readme",
  "dependencies": {
    "d3": "latest",
    "webworker-threads": "latest"
  },
  "devDependencies": {
    "babel-cli": "latest",
    "babel-eslint": "latest",
    "babel-plugin-transform-object-rest-spread": "latest",
    "babel-preset-env": "latest",
    "coveralls": "latest",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "latest",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "latest",
    "package-preamble": "0.1",
    "rollup": "0.52"
  },
  "jest": {
    "coverageDirectory": "<rootDir>/tests/coverage",
    "verbose": true
  }
}
